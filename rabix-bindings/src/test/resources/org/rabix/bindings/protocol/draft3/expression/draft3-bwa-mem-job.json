{
	"inputs": 
	{
		"reference": 
		{
			"class": "File",
			"path": "rabix/tests/test-files/chr20.fa",
			"size": 123,
			"checksum": "sha1$hash"
		},

		"reads": 
		[
			{
				"class": "File",
				"path": "rabix/tests/test-files/example_human_Illumina.pe_1.fastq"
			},

			{
				"class": "File",
				"path": "rabix/tests/test-files/example_human_Illumina.pe_2.fastq"
			}
		],

		"min_std_max_min": 
		[
			1,
			2,
			3,
			4
		],

		"minimum_seed_length": 3
	},

	"app": 
	{
		"id": "app@some_id^",
		"class": "CommandLineTool",
		"hints": 
		[
			{
				"class": "sbg:AWSInstanceType",
				"value": "c1.xlarge"
			},

			{
				"class": "sbg:MemRequirement",
				"value": 5000
			},

			{
				"class": "DockerRequirement",
				"dockerPull": "images.sbgenomics.com/rabix/bwa:9d3b9b0359cf",
				"dockerImageId": "9d3b9b0359cfa22f7eadd0587a82c9b066e384b9c6201d38427233f9ebf3b1aa"
			}
		],

		"requirements": 
		[
			{
				"class": "sbg:CPURequirement",
				"value": 7
			}
		],

		"inputs": 
		[
			{
				"id": "#reference",
				"type": 
				{
					"type": "File"
				},

				"inputBinding": 
				{
					"position": 2,
					"valueFrom": 
					{
						"engine": "javascript",
						"script": "${ return inputs.reference.path + '.tmp' }"
					}
				}
			},

			{
				"id": "#reads",
				"type": 
				{
					"type": "array",
					"items": "File"
				},

				"inputBinding": 
				{
					"position": 3
				}
			},

			{
				"id": "#reads",
				"type": 
				[
					{
						"type": "array",
						"items": "File"
					}
				]
			},

			{
				"id": "#minimum_seed_length",
				"type": 
				{
					"type": "int"
				},

				"inputBinding": 
				{
					"position": 1,
					"prefix": "-m"
				}
			},

			{
				"id": "#min_std_max_min",
				"type": 
				{
					"items": "int",
					"type": "array"
				},

				"inputBinding": 
				{
					"itemSeparator": ",",
					"position": 1,
					"prefix": "-I"
				}
			}
		],

		"outputs": 
		[
			{
				"id": "#sam",
				"type": 
				{
					"type": "File"
				},

				"outputBinding": 
				{
					"glob": "output.sam",
					"sbg:metadata": 
					{
						"file_type": "bam"
					}
				}
			}
		],

		"arguments": 
		[
			{
				"valueFrom": "$(inputs['min_std_max_min'][0])",
				"position": 1,
				"prefix": "-t"
			}
		],

		"baseCommand": 
		[
			"bwa",
			"mem"
		],

		"stdout": "output.sam",
		"stdin": "input.txt"
	}
}